# -*- coding: utf-8 -*-
import requests
import sys
base_domin='http://mm131.com/xinggan/'

for i in range(10000):
         if i < 100:
            continue
         urll=base_domin+str(i)+".html"
         r = requests.get(urll)
         if not r.status_code == 404:
             print(r.status_code)
         fo = open("xinggan.log", "a")
         fo.write(str(r.status_code))
         fo.write('\n')
         fo.close()
